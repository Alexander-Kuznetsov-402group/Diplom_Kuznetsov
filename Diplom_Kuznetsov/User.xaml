<Window x:Class="Diplom_Kuznetsov.User"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diplom_Kuznetsov"
        mc:Ignorable="d"
        Title="Окно пользователя" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="390"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <DataGrid x:Name="DStrah" IsReadOnly="True" AutoGenerateColumns="False" Grid.Row="0">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Номер страховки"/>
                <DataGridTextColumn Header="Название страховки"/>
                <DataGridTextColumn Header="Описание страховки"/>
                <DataGridTextColumn Header="Период действия страховки"/>
                <DataGridTextColumn Header="Стоимость страховки"/>
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Вернуться на окно авторизации" x:Name="Back" Grid.Row="1" Height="30" Width="200" Click="Back_Click"/>
    </Grid>
</Window>
